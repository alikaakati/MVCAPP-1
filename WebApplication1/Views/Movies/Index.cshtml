@model WebApplication1.ViewModel.ListViewModel

@{
    ViewBag.Title = "My movie";
}
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Month</th>
            <th>Year</th>
        </tr>
    </thead>
    <tbody>


        @foreach (var movie in Model.ListOfMovies)
        {   
        <tr>
            <td><a href="/Movies/Details/@movie.movieID">@movie.movieID</a></td>
            <td>@movie.name</td>
            <td>@movie.month</td>
            <td>@movie.year</td>
        </tr>

        }
    </tbody>
</table>

